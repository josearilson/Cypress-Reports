{
  "reporterEnabled": "mochawesome",
  "mochawesomeReporterOptions": {
    "reportDir": "cypress/reports",
    "charts": true,
    "reportPageTitle": "My Test Suite",
    "embeddedScreenshots": true,
    "inlineAssets": true,
    "overwrite": true,
    "json": true,
    "embeddedVideo": true,
    "autoOpen": true,
    "html": true,
    "reportJsonFile": "cypress/reports/custom-data.json",
