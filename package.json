{
  "name": "cypress-api-tutorial",
  "version": "1.0.0",
  "description": "Repository created in the article REST API Tests with Cypress",
  "main": "index.js",
  "scripts": {
    "cypress:open": "cypress open --config-file=cypress.config.js",
    "cy:run": "npx cypress run",
      "report:merge": "mkdir -p cypress/reports && npx marge --reportDir cypress/reports --reportFilename mochawesome-report --saveJson",
  
    "report:mocha": "marge cypress/reports/mochawesome-report.json --reportDir cypress/reports",
   
   
    "report:clean": "rimraf mochawesome-report index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/murillowelsi/cypress-api-tutorial.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/murillowelsi/cypress-api-tutorial/issues"
  },
  "homepage": "https://github.com/murillowelsi/cypress-api-tutorial#readme",
  "devDependencies": {
    "cypress-multi-reporters": "^1.6.0",
    "faker": "^5.5.3",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "cypress": "^10.9.0",
    "cypress-plugin-api": "^1.2.3"
  }
}
